# psycopg (v3) 드라이버를 명시적으로 지정
DATABASE_URL="postgresql+psycopg://banduser:bandpassword@db:5432/banddb"

# 중요: 아래 값은 예시이며, 실제 .env 파일에는 openssl rand -hex 32 명령어로 생성한 자신만의 키를 사용
JWT_SECRET_KEY="change-this-to-your-own-secret"
JWT_ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=60
REFRESH_TOKEN_EXPIRE_DAYS=30
